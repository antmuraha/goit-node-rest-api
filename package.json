{
    "name": "goit-node-rest-api",
    "type": "module",
    "scripts": {
        "start": "NODE_ENV=production node ./app.js",
        "dev": "NODE_ENV=development nodemon ./app.js",
        "db:migrate": "sequelize-cli db:migrate",
        "db:migrate:undo": "sequelize-cli db:migrate:undo",
        "db:seed": "sequelize-cli db:seed:all",
        "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.4",
        "express": "^5.2.1",
        "gravatar": "^1.8.2",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.3",
        "morgan": "^1.10.0",
        "multer": "^2.1.0",
        "nodemailer": "^8.0.1",
        "pg": "^8.11.3",
        "sequelize": "^6.37.7",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "nodemon": "^3.1.14",
        "sequelize-cli": "^6.6.5"
    },
    "pnpm": {
        "overrides": {
            "minimatch": "^9.0.7",
            "dottie": "^2.0.7",
            "qs": "^6.14.2"
        }
    }
}
